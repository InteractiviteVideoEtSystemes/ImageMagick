prefix=/usr
exec_prefix=${prefix}
libdir=/usr/lib64
includedir=/usr/include/ImageMagick-6
includearchdir=/usr/include/ImageMagick-6
libname=MagickCore-6.Q16
moduledir=/usr/lib64/ImageMagick-6.8.7/modules-Q16

Name: MagickCore
Description: MagickCore - C API for ImageMagick (ABI Q16)
Version: 6.8.7
Libs: -L${libdir} -l${libname}
Libs.private: -L${libdir} -l${libname} -lm
Cflags: -I${includearchdir} -I${includedir} -fopenmp -DMAGICKCORE_HDRI_ENABLE=0 -DMAGICKCORE_QUANTUM_DEPTH=16
